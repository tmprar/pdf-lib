{
  "name": "pdf-lib",
  "version": "1.17.1",
  "description": "Create and modify PDF files with JavaScript",
  "author": "Andrew Dillon <andrew.dillon.j@gmail.com>",
  "contributors": [
    "jerp (https://github.com/jerp)",
    "Greg Bacchus (https://github.com/gregbacchus)",
